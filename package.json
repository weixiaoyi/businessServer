    "babel-plugin-add-module-exports": "^1.0.2",
    "bip39": "^3.0.2",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.1",
    "fastscan": "^1.0.6",
    "helmet": "^3.21.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "lodash": "^4.17.11",
    "lru-cache": "^5.1.1",
    "method-override": "^3.0.0",
